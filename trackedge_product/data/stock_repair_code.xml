	<record id="stock_repair_code_G001" model="stock.repair.code">
        <field name="name">G001</field>
        <field name="description">Replaced Component (has no Serial#)</field>
        <field name="action_taken">Replacement of faulty (or suspected) component that may have affected the Units functionality</field>
        <field name="status">rok</field>
    </record>
	<record id="stock_repair_code_G002" model="stock.repair.code">
        <field name="name">G002</field>
        <field name="description">Replaced Part (has no Serial#)</field>
        <field name="action_taken">Replacement of faulty (or suspected) part that may have affected the Units functionality</field>
        <field name="status">rok</field>
    </record>
	<record id="stock_repair_code_G003" model="stock.repair.code">
        <field name="name">G003</field>
        <field name="description">Replaced Sub Unit (has Serial#)</field>
        <field name="action_taken">Replacement of faulty (or suspected) Sub Unit that may have affected the Units functionality</field>
        <field name="status">rok</field>
    </record>
	<record id="stock_repair_code_G004" model="stock.repair.code">
        <field name="name">G004</field>
        <field name="description">Replaced Module (has Serial#)</field>
        <field name="action_taken">Replacement of faulty (or suspected) Module that may have affected the Units functionality</field>
        <field name="status">rok</field>
    </record>
	<record id="stock_repair_code_G005" model="stock.repair.code">
        <field name="name">G005</field>
        <field name="description">Aligned / Adjusted / Trimmed</field>
        <field name="action_taken">Alignment / adjustment / trimming activity electrical and / or mechanical executed</field>
        <field name="status">rok</field>
    </record>
	<record id="stock_repair_code_G006" model="stock.repair.code">
        <field name="name">G006</field>
        <field name="description">Repaired / Refurbished (No part Replaced)</field>
        <field name="action_taken">Correction / Refurbish / Repair of fault / problem that may have affected the Units functionality</field>
        <field name="status">rok</field>
    </record>
	<record id="stock_repair_code_G007" model="stock.repair.code">
        <field name="name">G007</field>
        <field name="description">Modified / Upgraded Unit / Sub Unit</field>
        <field name="action_taken">Upgrade / Modification of handled Unit (ECN) (Based on Product Design documentation)</field>
        <field name="status">rok</field>
    </record>
	<record id="stock_repair_code_G008" model="stock.repair.code">
        <field name="name">G008</field>
        <field name="description">Reprogrammed</field>
        <field name="action_taken">Units Software / Firmware / Configuration corrected</field>
        <field name="status">rok</field>
    </record>
	<record id="stock_repair_code_C001" model="stock.repair.code">
        <field name="name">C001</field>
        <field name="description">Cosmetic Replacement </field>
        <field name="action_taken">Replacement of component / part / consumable (i.e. dust cover) that did not affect the Units functionality or reason to be replaced </field>
        <field name="status">rok</field>
    </record>
	<record id="stock_repair_code_B001" model="stock.repair.code">
        <field name="name">B001</field>
        <field name="description">Scrapped / Unrepairable Unit (BER)</field>
        <field name="action_taken">Unit is classified technically as unrepairable, Unit will be scrapped at RC or returned to warehouse for Scrapping</field>
        <field name="status">ber</field>
    </record>
	<record id="stock_repair_code_B002" model="stock.repair.code">
        <field name="name">B002</field>
        <field name="description">Rejected / Unrepairable Unit (Damaged)</field>
        <field name="action_taken">Unit is classified technically as unrepairable, Unit will be Rejected. Mechanical defect, due to external cause and returned to End Customer</field>
        <field name="status">ber</field>
    </record>
	<record id="stock_repair_code_B003" model="stock.repair.code">
        <field name="name">B003</field>
        <field name="description">Harvested /Unrepairable (BER)</field>
        <field name="action_taken">Unit is classified technically as unrepairable, components have been harvested for components</field>
        <field name="status">ber</field>
    </record>
	<record id="stock_repair_code_N001" model="stock.repair.code">
        <field name="name">N001</field>
        <field name="description">NO Repair Refurb Required</field>
        <field name="action_taken">This is an inventory item that has not passed through a TRU</field>
        <field name="status">nff</field>
    </record>
	<record id="stock_repair_code_N002" model="stock.repair.code">
        <field name="name">N002</field>
        <field name="description">No Fault Found (NFF-Screening) in Screening / Verification Test</field>
        <field name="action_taken">Unit fully tested at repair centre, No functional Fault was Found - Prior to Repair attempt. Return to WH</field>
        <field name="status">nff</field>
    </record>
	<record id="stock_repair_code_U001" model="stock.repair.code">
        <field name="name">U001</field>
        <field name="description">Unit Faulty Initial Repair  not concluded</field>
        <field name="action_taken">Screening / Verification Test executed, Unit found faulty - unit under repair</field>
        <field name="status">faulty</field>
    </record>
	<record id="stock_repair_code_U002" model="stock.repair.code">
        <field name="name">U002</field>
        <field name="description">Unit Faulty Failed Screening or Repair was not concluded</field>
        <field name="action_taken">Screening / Verification Test executed, Unit found faulty or Repair Centre could not finalize the repair of the Unit, Unit may be returned to warehouse and/or 2nd tier Repair Centre</field>
        <field name="status">faulty</field>
    </record>
</data>
</odoo>